#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules/react-email/dist/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules/react-email/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules/react-email/dist/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules/react-email/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/react-email@5.1.1/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/index.js" "$@"
fi
