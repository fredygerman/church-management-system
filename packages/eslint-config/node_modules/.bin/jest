#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/bin/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/bin/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules:/Users/fredygerman/Personal/builds/church/church-management-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/jest@30.2.0_esbuild-register@3.6.0_esbuild@0.27.2_/node_modules/jest/bin/jest.js" "$@"
fi
